<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace 表示对应的哪个文件
    id表示对应的哪个方法-->
<mapper namespace="com.azy.mapper.UserMapper">
    <select id="selectAll" resultType="com.azy.pojo.User">
        select *
        from tb_user;
    </select>
</mapper>